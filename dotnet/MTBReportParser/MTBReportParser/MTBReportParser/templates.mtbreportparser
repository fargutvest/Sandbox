_.Where(_ => _.DateTime >= new DateTime(2021, 03, 01) && _.DateTime < new DateTime(2021, 04, 01) && _.Amount < 0 && _.Status == Status.Success).GroupBy(_ => _.Place).ToList().ToDictionary(_ => _.Key, _ => $" ({string.Join("; ", _.Select(x => $"{x.Title} {x.Amount}"))}) {_.Sum(x => x.Amount)}").Select(_ => $"[{_.Key}]: {_.Value}").ToList()
